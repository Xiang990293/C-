#include<iostream>
using namespace std;

int main(){
	int n=0;
	while(cin >> n){
		for(int i=0; i<n; i++){
			int O=0,fac=1;
			cin >> O;
			bool is_still_zero=true;
			if(O==0){
				cout << 0 << endl;
			}else{ 
				for(int j=9; j>=0; j--){
					int m=1;
					for(int k=0; k<j; k++) m*=10;
					if(O/m!=0) is_still_zero = false;
					if(!is_still_zero){
						fac*=O/m;
						O-=m*(O/m);
					}
				}
				cout << fac << endl;
			} 
		}
	}
}
